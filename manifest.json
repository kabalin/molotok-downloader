{
    "name": "Molotok Downloader",
    "version": "0.1",
    "description": "Chrome extension for downloading images from molotok.ru auction website.",
    "manifest_version": 2,
    "content_scripts": [ {
        "js": [ "script.js" ],
        "css": ["script.css"],
        "matches": [
            "http://molotok.ru/*"
        ],
        "run_at": "document_idle"
    } ],
    "permissions": [
        "http://molotok.ru/*",
        "downloads"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "options_page": "options.html"
}
